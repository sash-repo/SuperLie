Cell["EnumPoint", "ObjectName",
 CellTags->{"EnumPoint", "b:9.1"}],

Cell[TextData[{
 "\[FilledSmallSquare] ",
 StyleBox["EnumPoint", "Program"],
 "[",
 StyleBox["var",
  FontSlant->"Italic"],
 ", ",
 StyleBox["set",
  FontSlant->"Italic"],
 ", ",
 StyleBox["elt",
